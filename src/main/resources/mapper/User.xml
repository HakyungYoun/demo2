<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
	<mapper namespace="User">
		<insert id="User_insert" parameterType="UserDto">
			insert into user
			(
				user_id
				,user_nm
				,user_eml
				,user_dept_no
				,user_telno
				,user_addr
			)
			values
			(
				#{user_id}
				,#{user_nm}
				,#{user_eml}
				,#{user_dept_no}
				,#{user_telno}
				,#{user_addr}
			)
		</insert>

	</mapper>